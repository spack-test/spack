mirrors:
  buildcache-source:
    fetch: ${PROTECTED_MIRROR_FETCH_DOMAIN}/testing/${PR_TARGET_REF_NAME}/${SPACK_CI_STACK_NAME}
    push: ${PROTECTED_MIRROR_PUSH_DOMAIN}/testing/${PR_TARGET_REF_NAME}/${SPACK_CI_STACK_NAME}
    source: False
    binary: True
  buildcache-destination:
    fetch: ${PR_MIRROR_FETCH_DOMAIN}/testing/${CI_COMMIT_REF_NAME}/${SPACK_CI_STACK_NAME}
    push: ${PR_MIRROR_PUSH_DOMAIN}/testing/${CI_COMMIT_REF_NAME}/${SPACK_CI_STACK_NAME}
    source: False
    binary: True
  buildcache-shared:
    fetch: ${PR_MIRROR_FETCH_DOMAIN}/testing/shared_pr_mirror/${SPACK_CI_STACK_NAME}
    push: ${PR_MIRROR_PUSH_DOMAIN}/testing/shared_pr_mirror/${SPACK_CI_STACK_NAME}
    source: False
    binary: True
